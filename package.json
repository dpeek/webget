{
  "name": "webget",
  "version": "0.1.0",
  "type": "module",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "bin": {
    "wg": "./bin/wg"
  },
  "dependencies": {
    "playwright": "^1.42.0"
  },
  "devDependencies": {
    "@elysiajs/eden": "^1.0.7",
    "@elysiajs/static": "^1.0.2",
    "@types/bun": "latest",
    "@types/listr": "^0.14.9",
    "@types/yargs": "^17.0.32",
    "any-observable": "^0.5.1",
    "elysia": "^1.0.9",
    "listr2": "^8.1.1",
    "prettier": "^3.2.5",
    "typescript": "^5.2.0",
    "yargs": "^17.7.2",
    "zen-observable": "^0.10.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4",
    "zod-to-ts": "^1.2.0"
  },
  "files": [
    "bin/wg",
    "src/index.js",
    "src/index.d.ts",
    "public/"
  ],
  "scripts": {
    "postinstall": "playwright install",
    "clean": "bun src/scripts/clean.ts",
    "schema": "bun src/scripts/schema.ts",
    "types": "bun src/scripts/types.ts",
    "dev": "bun --watch src/dev.ts",
    "wg": "bun src/cli.ts",
    "build": "bun build --compile --external electron --sourcemap ./src/cli.ts --outfile wg && mkdir -p bin && mv src/wg bin/wg",
    "pack": "npm pack --pack-destination ../tester"
  }
}
